<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->




<bean class="com.springcore.com.springcore.Employee" name="emp">

<property name="eid" value="124"/>
<property name="eName" value="Jitesh Binnar"/>
</bean>

<bean class="com.springcore.com.springcore.Laptop" name="lap">

<constructor-arg name="lid" value="123"/>

<constructor-arg name="modelname" value="Hp"/>


</bean>




</beans>